  "scripts": {
    "serve": "npm run build && serve -s .\\build\\",
    "start": "react-scripts --openssl-legacy-provider start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "electron": "electron .",
